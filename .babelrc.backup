{
  "presets": ["env", "flow", "react"],
  "plugins": [
    "syntax-dynamic-import", // client side import
    "dynamic-import-node", // server side import
    "babel-plugin-inline-import",
    "relay",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    [
      "import",
      {
        "libraryName": "lodash"
      }
    ]
  ]
}

{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-regenerator",
    "import-graphql",
    "relay",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "@material-ui/core"
      },
      "material-ui"
    ]
  ]
}
